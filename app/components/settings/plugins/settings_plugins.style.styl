default_color = #58666e

.list_number_point
  padding-left 0
  list-style-type none
  
  li
    clear both
    line-height 28px

    span.number_point
      margin 3px 15px 0 0
      float left
      border 1px solid default_color
      border-radius 50%
      width 22px
      height 22px
      line-height 20px
      text-align center
